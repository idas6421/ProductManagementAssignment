        "@storybook/addon-links": "^6.0.28",
        "@storybook/angular": "^6.0.28",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.11.1",
        "babel-loader": "^8.2.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^26.6.3",
        "jest-preset-angular": "^8.3.2",
        "json-server": "^0.16.2",
        "json-server-auth": "^2.0.2",
        "protractor": "~5.4.3",
